<h1>Sign Up</h1>


<%= form_for (@adminUser), url: sign_up_path do |f| %>

    <% if @adminUser.errors.any? %>
    <% @adminUser.errors.full_messages.each do |msg| %>
        <%= msg%><br>
        <%end%>
    <%end%>

<table>
    <tr>
        <th>    <%= f.label :first_name %></th>
        <td>    <%= f.text_field :first_name %></td>
    </tr>
    <tr>
        <th>    <%= f.label :last_name %></th>
        <td>    <%= f.text_field :last_name %></td>
    </tr>
    <tr>
        <th>    <%= f.label :username %></th>
        <td>    <%= f.text_field :username %></td>
    </tr>
    <tr>
        <th>    <%= f.label :email %></th>
        <td>    <%= f.text_field :email %></td>
    </tr>
    <tr>
        <th>    <%= f.label :password %></th>
        <td>    <%= f.password_field :password %></td>
    </tr>
    <tr>
        <th>    <%= f.label :password_confirmation %></th>
        <td>    <%= f.password_field :password_confirmation %></td>
    </tr>
</table>

    <%=f.submit%>
    <%end%>